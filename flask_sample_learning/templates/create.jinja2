{% extends 'base.jinja2' %}

{% block content %}
<h1>Create post</h1>
<form method="POST">
	<div>
		<label for="title">Title</label>
		<input type="text" name="title" id="title" />
	</div>

	<div>
		<label for="content">Content</label>
		<textarea name="content" id="content"></textarea>
	</div>

	<div>
		<input type="submit">
	</div>
</form>
{% endblock %}